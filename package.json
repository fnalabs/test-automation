{
  "name": "test-automation",
  "version": "2.0.6",
  "description": "Scaffolding on top of Protractor to follow a Page Object pattern for Automated browser tests.",
  "keywords": [
    "auto",
    "automation",
    "protractor",
    "selenium",
    "test",
    "testing",
    "webdriver",
    "webdriverjs"
  ],
  "main": "index.js",
  "scripts": {
    "prebuild": "del dist",
    "build": "NODE_ENV=production bash ./bin/build",
    "coverage": "NODE_ENV=test nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "dev": "nodemon -e js,json -w src -w conf -x \"npm run build\" & nodemon -e js -w tst -x \"npm test\"",
    "document": "documentation build -c documentation.yml -f html -o docs/ src/**",
    "fix": "standard --fix",
    "prerelease": "del package.tar.gz",
    "release": "NODE_ENV=production npm test && npm run build && node ./bin/compress",
    "postrelease": "npm run document",
    "pretest": "del coverage coverage.lcov .nyc_output",
    "test": "standard",
    "posttest": "NODE_ENV=test nyc --reporter=html --reporter=text mocha --recursive --exit 'tst/**/*.spec.js'",
    "version": "npm run release && git add --all",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fnalabs/test-automation.git"
  },
  "bugs": {
    "url": "https://github.com/fnalabs/test-automation/issues"
  },
  "homepage": "https://fnalabs.github.io/test-automation/",
  "author": "Adam Eilers",
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "dependencies": {
    "chai": "~4.1.2",
    "chai-as-promised": "~7.1.1",
    "dirty-chai": "~2.0.1"
  },
  "devDependencies": {
    "archiver": "~2.1.1",
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.3",
    "babel-eslint": "~8.2.3",
    "babel-plugin-add-module-exports": "~0.2.1",
    "babel-plugin-istanbul": "~4.1.6",
    "babel-plugin-transform-es2015-modules-commonjs": "~6.26.2",
    "babel-register": "~6.26.0",
    "codecov": "~3.0.2",
    "del-cli": "~1.1.0",
    "documentation": "~8.0.0",
    "mocha": "~5.2.0",
    "nodemon": "~1.17.5",
    "nyc": "~12.0.2",
    "proxyquire": "~2.0.1",
    "sinon": "~6.0.0",
    "standard": "~11.0.1",
    "uglify-es": "~3.3.9"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "watermarks": {
      "lines": [
        80,
        90
      ],
      "functions": [
        80,
        90
      ],
      "branches": [
        80,
        90
      ],
      "statements": [
        80,
        90
      ]
    },
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "standard": {
    "ignore": [
      "docs/"
    ],
    "parser": "babel-eslint"
  }
}
